{
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org",
    "only": ["wpackagist-plugin/*", "wpackagist-theme/*"]
  }],
  "require": {
    "htmlburger/carbon-fields": "^3.0",
    "wpackagist-plugin/akismet": "^4.1",
    "wpackagist-plugin/better-search-replace": "^1.3",
    "wpackagist-plugin/classic-editor": "^1.6",
    "wpackagist-plugin/contact-form-7": "^5.3",
    "wpackagist-plugin/contact-form-7-dynamic-text-extension": "^2.0",
    "wpackagist-plugin/mailpoet": "^3.55",
    "wpackagist-plugin/profile-builder": "^3.3",
    "wpackagist-plugin/svg-support": "^2.3",
    "wpackagist-plugin/wp-user-avatar": "^2.2",
    "wpackagist-plugin/wp-recall": "^16.24",
    "wpackagist-plugin/duplicate-post": "^3.2"
  },
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
    }
  }
}
